#include <stdio.h>

int main () {
    int N, v, count = 0;
    int num[100];

    scanf("%d", &N);
    for(int i =0;i<N; i++) {
        scanf("%d", &num[i]);
    }
    scanf("%d", &v);
    
    for(int j=0;j<N;j++){
        if(num[j] == v) {
            count++;
        }
    }
    
    printf("%d \n", count);
    return 0;
}
