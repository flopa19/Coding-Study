#include <stdio.h>

int main () {
    // 10의 12승까지의 수를 입력받고 계산하여 출력하기 때문에 long long형으로 입출력을 받아야 함.
    long long a,b,c;
    scanf("%lld %lld %lld", &a, &b, &c);

    // 입력 범위가 1부터 시작이기 때문에 음수이거나 0일경우 강제종료.
    if(a>0 && b>0 && c>0) {
        printf("%lld\n", a+b+c);
    }
    else return -1;
    return 0;
}
