#include <stdio.h>

int main () {
    int X, N, a, b;
    int result = 0;

    scanf("%d", &X);
    //구매한 물건의 종류의 수
    scanf("%d", &N);
    // 종류의 수만큼 입력
    for(int i=0; i<N; i++) {
        scanf("%d %d", &a, &b);
        result += a*b;
    }
    
    if(result == X)
        printf("Yes \n");
    else 
        printf("No \n");
    
    return 0;
}
